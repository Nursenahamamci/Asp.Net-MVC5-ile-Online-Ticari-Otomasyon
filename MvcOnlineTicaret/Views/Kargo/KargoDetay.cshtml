@using MvcOnlineTicaret.Models.Siniflar
@model List<KargoTakip>


@{
    ViewBag.Title = "KargoDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>KargoDetay</h2>


<table class="table table-hover">
    <tr>

        <th> AÇIKLAMA</th>
        <th> TARİH</th>
        
    </tr>
    @foreach (var k in Model)
    {
        <tr>

            <td>@k.Aciklama</td>
            <td>@k.TarihZaman</td>

           
        </tr>
    }

</table>